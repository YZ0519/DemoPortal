<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>DemoPortal - @ViewData["Title"]</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <!-- Favicons -->
    <link href="~/lib/assets/img/favicon.png" rel="icon">
    <link href="~/lib/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link rel="stylesheet" href="~/lib/assets/vendor/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/lib/assets/vendor/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/lib/assets/vendor/vendors/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="~/lib/assets/vendor/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/lib/assets/vendor/vendors/owl-carousel-2/owl.carousel.min.css">
    <link rel="stylesheet" href="~/lib/assets/vendor/vendors/owl-carousel-2/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/lib/assets/vendor/css/style.css">
    <!-- Template Main CSS File -->

    <link href="~/css/sweetalert2-dark.css" rel="stylesheet">
    <link href="~/css/select2.min.css" rel="stylesheet">
    <link href="~/css/dataTables.bootstrap5.min.css" rel="stylesheet">
    <link href="~/css/select2-bootstrap-5-theme.min.css" rel="stylesheet">
    @await RenderSectionAsync("Css", required: false)
</head>
<body>
    <main>
        <div class="container">
            <section class="section register min-vh-100 d-flex flex-column align-items-center justify-content-center py-4">
                <div class="container">
                    <div class="row justify-content-center">
                        <div class="col-lg-4 col-md-6 d-flex flex-column align-items-center justify-content-center">

                            <div class="d-flex justify-content-center py-4">
                                <a href="#" class="logo d-flex align-items-center w-auto p-0 sidebar-brand brand-logo text-decoration-none">
                                    <img src="~/lib/assets/img/logo.png" alt="">
                                    <span class="d-none d-lg-block text-white">Demo Portal</span>
                                </a>
                            </div><!-- End Logo -->
                            @RenderBody()
                        </div>
                    </div>
                </div>
                <br />
                <div class="copyright">
                    &copy; Copyright <strong><span>DemoPortal</span></strong> 2024. All Rights Reserved
                </div>
                <div class="credits">
                    <small>
                        Designed by <a href="https://www.bootstrapdash.com/bootstrap-admin-template/">Bootstrapdash.com</a>
                    </small>
                </div>
            </section>
        </div>
    </main><!-- End #main -->
    <!-- page-body-wrapper ends -->
    <!-- Vendor JS Files -->
    <script src="~/lib/assets/vendor/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/lib/assets/vendor/vendors/chart.js/Chart.min.js"></script>
    <script src="~/lib/assets/vendor/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/lib/assets/vendor/vendors/jvectormap/jquery-jvectormap.min.js"></script>
    <script src="~/lib/assets/vendor/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/lib/assets/vendor/vendors/owl-carousel-2/owl.carousel.min.js"></script>
    <script src="~/lib/assets/vendor/js/jquery.cookie.js" type="text/javascript"></script>
    <script src="~/lib/assets/vendor/js/off-canvas.js"></script>
    <script src="~/lib/assets/vendor/js/hoverable-collapse.js"></script>
    <script src="~/lib/assets/vendor/js/misc.js"></script>
    <script src="~/lib/assets/vendor/js/settings.js"></script>
    <script src="~/lib/assets/vendor/js/todolist.js"></script>
    <script src="~/lib/assets/vendor/js/dashboard.js"></script>

    <!-- Template Main JS File -->
    <script src="~/js/sweetalert2.min.js"></script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @* <script src="~/js/active-configuration.js" asp-append-version="true"></script> *@
    <script src="~/js/select2.full.min.js"></script>

    <script src="~/js/jquery.dataTables.min.js" asp-append-version="true"></script>
    <script src="~/js/dataTables.bootstrap5.min.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <script>
        if ('@TempData["ErrorMsg"]') {
            Swal.fire({
                title: "Error",
                text: "@TempData["ErrorMsg"]",
                icon: "error"
            });
        }
    </script>
</body>
</html>
